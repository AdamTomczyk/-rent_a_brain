
<%= render "shared/navbar" %>
<div class="container">
  <div class="row mt-5">
    <div class="col-12">
    <h1 class="mt-5 mb-5" style="color:black">Book your Brain</h1>
    <%= simple_form_for [ @brain, @booking ] do |f| %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.submit "Book this brain", class: "btn btn-primary mt-3", data: { confirm: 'Are you sure you want to book this brain?' } %>


<% end %>
</div>
  </div>
</div>
